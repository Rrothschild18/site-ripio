{
	"name": "MOGLIBASE",
	"author": "Raul Rothschild",
	"license": "ISC",
	"version": "1.0.0",
	"description": "Learning config projects",
	"main": "gulpfile.js",
	"copyright": "FREE",
	"private": true,
	"paths": {
		"src": {
			"styles": "./src/styles/",
			"stylesDist": "./src/styles/dist/",
			"fontello": "./src/fonts/fontello/",
			"js": "./src/js/",
			"img": "./src/images/"
		},
		"build": {
			"css": "./assets/css/",
			"fontello": "./assets/fonts/fontello/",
			"fonts": "./assets/fonts/",
			"js": "./assets/js/",
			"img": "./assets/images/"
		}
	},
	"vars": {
		"siteJsName": "app.js",
		"siteCssName": "app.css",
		"scssName": "main.scss"
	},
	"globs": {
		"distCss": [
			"./src/styles/libs/*.css",
			"./src/styles/dist/main.css"
		],
		"distJs": [
			"./src/js/main.js",
			"./src/js/libs/*.js"
		],
		"img": [
			"./public/images/"
		]
	},
	"devDependencies": {
		"browser-sync": "^2.26.12",
		"fancy-log": "^1.3.3",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^3.1.0",
		"gulp-cached": "^1.1.1",
		"gulp-concat": "^2.6.0",
		"gulp-filter": "^5.0.1",
		"gulp-header": "^1.8.7",
		"gulp-if": "^2.0.1",
		"gulp-imagemin": "^7.1.0",
		"gulp-livereload": "^4.0.2",
		"gulp-load-plugins": "^1.3.0",
		"gulp-plumber": "^1.1.0",
		"gulp-cssnano": "2.1.3",
		"gulp-print": "^2.0.1",
		"gulp-rename": "^1.2.2",
		"gulp-rev": "^7.1.0",
		"gulp-sass": "^4.1.0",
		"gulp-sass-glob-import": "^0.1.0",
		"gulp-size": "^2.1.0",
		"gulp-sourcemaps": "^2.2.1",
		"gulp-uglify": "^1.5.4",
		"moment": "^2.27.0"
	},
	"dependencies": {
		"minimatch": "^3.0.4"
	}
}
